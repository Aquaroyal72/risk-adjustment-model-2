coinToss<-function(p,n){
# cointoss similates a simple coin toss.
# everytime it is called it returns 0 or 1,
# with equal probability 1/2.
x=runif(n,0,1)
for (i in n) {
if (x(i)>=0.5) x(i)=1 else x(i)=0
}
return x;
}
